<?php 
// get the name of a menu by 'location'
// e.g.
register_nav_menus( 
      [
      'primary_menu' => esc_html__( 'Nav Menu', 'codexpert-agency' ),
      'footer_menu_1'  => esc_html__( 'Footer Menu One', 'codexpert-agency' )
      ]
  );

// show the menu name
echo wp_get_nav_menu_name('primary_menu'); 
echo wp_get_nav_menu_name('footer_menu_1'); 
?>
