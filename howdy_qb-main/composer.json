{
    "name": "codesvault/howdy-qb",
    "description": "Database Query Builder for WordPress",
    "version": "0.0.1",
    "scripts": {
        "test": "phpunit",
		"phpcs": "phpcs ",
        "phpcbf": "phpcbf ",
		"post-package-install": "phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs"
    },
    "autoload": {
        "psr-4": {
            "CodesVault\\Howdyqb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodesVault\\Howdyqb\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "CodesVault",
            "email": "keramotul.islam@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
		"symfony/var-dumper": "^5.4",
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^2.3"
    },
    "keywords": [
        "wordpress",
        "database",
        "query",
        "builder",
        "mysql",
        "wp"
    ]
}
